                android:id="@+id/ll45"
                android:layout_width="50dp"
                android:layout_height="80dp"
                android:layout_alignParentRight="true"
                android:layout_below="@+id/ll35"
                android:layout_margin="5dp"
                android:background="#996656"
                android:orientation="horizontal">

                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:src="@mipmap/ic_launcher" />
            </LinearLayout>
        </RelativeLayout>

        <LinearLayout
            android:id="@+id/line5"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/line234"
            android:orientation="horizontal">

            <LinearLayout
                android:id="@+id/ll51"
                android:layout_width="50dp"
                android:layout_height="80dp"
                android:layout_margin="5dp"
                android:background="#996656"
                android:orientation="horizontal">

                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:src="@mipmap/ic_launcher" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/ll52"
                android:layout_width="50dp"
                android:layout_height="80dp"
                android:layout_margin="5dp"
                android:layout_toRightOf="@id/ll51"
                android:background="#996656"
                android:orientation="horizontal">

                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:src="@mipmap/ic_launcher" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/ll53"
                android:layout_width="50dp"
                android:layout_height="80dp"
                android:layout_margin="5dp"
                android:layout_toRightOf="@id/ll52"
                android:background="#996656"
                android:orientation="horizontal">

                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:src="@mipmap/ic_launcher" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/ll54"
                android:layout_width="50dp"
                android:layout_height="80dp"
                android:layout_margin="5dp"
                android:layout_toRightOf="@id/ll53"
                android:background="#996656"
                android:orientation="horizontal">

                <ImageView
